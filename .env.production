# Multiagent Consensus - Production Environment Variables
# =================================================
# Public variables specific to the production environment
# These values are safe to commit to version control

# Production Settings
# ------------------
# Disable verbose logging in production
DEBUG=false
LOG_LEVEL=error

# Production API settings (placeholder - would be replaced in deployment)
NEXT_PUBLIC_API_URL=https://api.yourdomain.com

# Production-specific timeouts (optimized for performance)
DEFAULT_TIMEOUT_MS=15000
DEFAULT_MAX_RETRIES=3

# Production feature flags
ENABLE_MOCK_PROVIDERS=false
ENABLE_DEBUGGER=false

# Enable caching for better performance
ENABLE_RESPONSE_CACHE=true
CACHE_TTL_SECONDS=3600 