# Multiagent Consensus - PRIVATE Environment Variables
# ===============================================
# Copy this file to .env.local and add your private API keys
# IMPORTANT: Never commit your actual .env.local file to version control

# Provider API Keys (PRIVATE)
# --------------------------
# OpenAI
OPENAI_API_KEY=

# Anthropic
ANTHROPIC_API_KEY=

# Google
GOOGLE_API_KEY=

# Mistral
MISTRAL_API_KEY=

# Cohere
COHERE_API_KEY=

# Groq
GROQ_API_KEY=

# Amazon Bedrock (all three required)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=

# Azure OpenAI (both required)
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=

# Perplexity
PERPLEXITY_API_KEY=

# XAI
XAI_API_KEY=

# Deepseek
DEEPSEEK_API_KEY=

# TogetherAI
TOGETHER_API_KEY=

# Fireworks
FIREWORKS_API_KEY=

# Google Vertex AI
GOOGLE_APPLICATION_CREDENTIALS=

# Replicate
REPLICATE_API_TOKEN=

# Cerebras
CEREBRAS_API_KEY=

# Luma
LUMA_API_KEY=

