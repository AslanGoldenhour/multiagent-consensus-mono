# Multiagent Consensus - Development Environment Variables
# ==================================================
# Public variables specific to the development environment
# These values are safe to commit to version control

# Development Settings
# -------------------
# Enable verbose logging in development
DEBUG=true
LOG_LEVEL=debug

# Development API settings
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Development-specific timeouts (longer for debugging)
DEFAULT_TIMEOUT_MS=60000
DEFAULT_MAX_RETRIES=5

# Development feature flags
ENABLE_MOCK_PROVIDERS=true
ENABLE_DEBUGGER=true 